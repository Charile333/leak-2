# 方案B：AWS部署配置示例

# LeakRadar API配置
VITE_LEAKRADAR_API_KEY=your-leakradar-api-key
VITE_OTX_API_KEY=your-otx-api-key

# JWT配置
JWT_SECRET=your-jwt-secret-key-change-in-production

# SMTP配置
SMTP_HOST=smtp.sendgrid.net
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=apikey
SMTP_PASS=your-smtp-password

# AI分析配置
AI_API_KEY=your-ai-api-key
AI_MODEL=deepseek/deepseek-chat
AI_API_BASE=https://api.deepseek.com/v1

# 前端配置
VITE_BACKEND_URL=http://localhost:3001
VITE_TRENDRADAR_DASHBOARD_URL=http://localhost:8080/html/latest/current.html

# 白名单配置 - JSON格式的邮箱数组
WHITELISTED_USERS=["user1@example.com", "user2@example.com"]
